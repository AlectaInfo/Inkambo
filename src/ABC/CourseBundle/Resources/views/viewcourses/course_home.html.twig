
<div id="course_home">

    <ul class="navbar">
              {%for key, courses in dynamic_courses%}
        <li><a href ="">{{key}}</a></li>
        {%for course in courses%}
        <ul>
            <li><A HREF ="{{path('course_description')}}#{{course.getCourseId()}}">{{course.getTitle}}</A></li>
        </ul>
            {%endfor%}
                    {%endfor%}

    </ul>
    <h3> Top Rated Courses </h3>   
    {%for course in top_courses%}
            <li><a href ="">{{course.course_id}}</a></li>

                        {%endfor%}


