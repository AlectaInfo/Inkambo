{# empty Twig template #}
<html>

    <head>
      
  <style>
      div.list{
    
        padding: 19px 25px 0px 20px;
        margin: 0 auto 20px;
        display: block;
       background-color: #F7F7F7;
        padding: 5px 5px 5px 10px;
        margin: 0px auto 25px;
        max-width: 300px;
        border-radius: 2px;
        box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
               
  </style>        
        
    </head>

    <body>
        
        {%for entity in entities%}
           <div class="list">
             <p> 
             <strong>RPID</strong> : {{entity.rpid}}<br>
             <strong>Name</strong> :{{entity.firstname}} {{entity.lastname}}<br>
             <strong>Email</strong> :{{entity.email}}
             </p>
             
             <a href="{{ path('rsp_show', { 'id': entity.rpid }) }}">show</a>
             <a href="{{ path('rsp_edit', { 'id': entity.rpid }) }}">edit</a>
             <form action="{{path('rsp_show',{ 'id':entity.rpid })}}" method="POST" style="">
                    <input type="submit" value="View Profile" class="bt btn-primary btn-success">
             </form>
           </div>
        {% endfor%}
                
    </body>

</html>
