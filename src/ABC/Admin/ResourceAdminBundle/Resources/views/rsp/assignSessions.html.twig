<style>
          body {
        padding-top: 40px;
        padding-bottom: 40px;
        background-color: #f5f5f5;
      }

      .form-signin {
        max-width: 350px;
        padding: 19px 25px 0px 20px;
        margin: 0 auto 20px;
        background-color: #fff;
        border: 1px solid #e5e5e5;
        -webkit-border-radius: 5px;
           -moz-border-radius: 5px;
                border-radius: 5px;
        -webkit-box-shadow: 0 1px 2px rgba(0,0,0,.05);
           -moz-box-shadow: 0 1px 2px rgba(0,0,0,.05);
                box-shadow: 0 1px 2px rgba(0,0,0,.05);
      }
      .form-signin .form-signin-heading,
      .form-signin .checkbox {
        margin-bottom: 10px;
      }
      .form-signin input[type="text"],
      .form-signin input[type="password"],
      .form-signin input[type="email"],
      .form-signin input[type="time"],
      .form-signin input[type='number']
      {
        font-size: 12px;
        height: 20px;
        margin-bottom: 10px;
        padding: 2px 2px;
      }
      
    a{
        text-decoration: none;
        color: #0088cc;
    }
    .linespace{
          margin-left: 10px;
          margin-right: 10px;
          color: #000000;
          font-weight: 900;
      }
</style>        
<script>
    function popitup(url) {
	newwindow=window.open(url,'name','height=600,width=400,screenX=500,screenY=50');
	if (window.focus) {newwindow.focus()}
	return false;
    }
</script>    
<div class="form-signin">
<h2>Assign Sessions : </h2>

{%for course in courses%}

    {{course.course_id}}  - {{course.title}}<br><br>
    <a href="{{path('session_newGiven',{'courseId': course.course_id,'id': id})}}"> Assign A session</a>

{%endfor%}
</div>    